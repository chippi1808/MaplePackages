dd5:=x1*x2*x3*x4*x5+x1*x2*x6*x3*x4+x1*x4*x5*x2*x6+x5*x6*x2*x3*x4+x5*x6*x3*x4*x1+x1*x2*x5*x6*x3;
dd6:=x1*x2*x3*x4*x5*x6;
