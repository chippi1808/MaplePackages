hh:=[op({temp} minus {{}})];
h:=subs(tj2,hh);

temp:=0;
for i from 1 to nops(hh) do temp:=temp+hh[i] od;
temp:=[op(sort(temp,d1))];
temp1:=0;
for i from 1 to nops(temp) do temp1:=temp1+k||i*temp[i] od;
return temp1;

end:


